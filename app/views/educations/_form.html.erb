<%= simple_form_for(@education) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :school_name %>
    <%= f.input :course %>
    <%= f.input :field_of_study %>
    <%= f.input :year_graduated %>
    <%= f.association :profile %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
